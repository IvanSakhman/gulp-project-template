{
  "name": "gulp-template",
  "version": "0.0.0",
  "description": "Project template for gulp.js",
  "main": "gulpfile.js",
  "scripts": {
    "prepublish": "bower install",
    "start": "gulp",
    "build": "gulp build"
  },
  "keywords": [
    "gulp",
    "template"
  ],
  "author": "",
  "dependencies": {
    "coffee-script": "~1.7.1"
  },
  "devDependencies": {
    "bower": "~1.3.5",
    "browserify": "~4.1.11",
    "browserify-shim": "~3.6.0",
    "coffeeify": "~0.6.0",
    "ecstatic": "~0.5.3",
    "event-stream": "~3.1.5",
    "gulp": "~3.8.1",
    "gulp-angular-templatecache": "~1.1.3",
    "gulp-autoprefixer": "0.0.7",
    "gulp-jade": "~0.6.0",
    "gulp-livereload": "~2.1.0",
    "gulp-minify-css": "~0.3.5",
    "gulp-plumber": "~0.6.3",
    "gulp-rename": "~1.2.0",
    "gulp-streamify": "0.0.5",
    "gulp-stylus": "1.0.2",
    "gulp-uglify": "~0.1.0",
    "gulp-util": "~2.2.9",
    "tiny-lr": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.2"
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "angular-route": "./bower_components/angular-route/angular-route.js"
  },
  "browserify-shim": {
    "angular": "angular",
    "angular-route": "ngRoute"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  }
}
